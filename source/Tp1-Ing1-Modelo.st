Object subclass: #MantoGeologico	instanceVariableNames: 'nombre'	classVariableNames: ''	poolDictionaries: ''	category: 'Tp1-Ing1-Modelo'!!MantoGeologico methodsFor: 'Initialize' stamp: 'JuanVanecek 4/5/2017 19:29'!initializeConocidoComo: unNombre	nombre := unNombre! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!MantoGeologico class	instanceVariableNames: ''!!MantoGeologico class methodsFor: 'Instance Creation' stamp: 'JuanVanecek 4/5/2017 19:28'!conocidoComo: unNombre	^self new initializeConocidoComo: unNombre! !Object subclass: #ParcelaDeYacimiento	instanceVariableNames: 'presionInicial profundidadDeReservorio mantoGeologico'	classVariableNames: ''	poolDictionaries: ''	category: 'Tp1-Ing1-Modelo'!!ParcelaDeYacimiento methodsFor: 'Initialize' stamp: 'JuanVanecek 4/5/2017 19:21'!initializeConPresionInicial: unaMedidaDePresion 	yProfunidadDeMetros: unaMedidaDeDistancia		presionInicial := unaMedidaDePresion.	profundidadDeReservorio := unaMedidaDeDistancia! !!ParcelaDeYacimiento methodsFor: 'Initialize' stamp: 'JuanVanecek 4/5/2017 19:31'!initializeDeTipo: aMantoGeologico 	conPresionInicial: unaMedidaDePresion 	yProfunidadDeMetros: unaMedidaDeDistancia		mantoGeologico := aMantoGeologico.	presionInicial := unaMedidaDePresion.	profundidadDeReservorio := unaMedidaDeDistancia! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ParcelaDeYacimiento class	instanceVariableNames: ''!!ParcelaDeYacimiento class methodsFor: 'Instance Creation' stamp: 'JuanVanecek 4/5/2017 19:20'!conPresionInicial: unaMedidaDePresion yProfunidadDeMetros: unaMedidaDeDistancia	^self new 		initializeConPresionInicial: unaMedidaDePresion 		yProfunidadDeMetros: unaMedidaDeDistancia! !!ParcelaDeYacimiento class methodsFor: 'Instance Creation' stamp: 'JuanVanecek 4/5/2017 19:30'!deTipo: unMantoGeologico  	conPresionInicial: unaMedidaDePresion 	yProfunidadDeMetros: unaMedidaDeDistancia		^self new 		initializeDeTipo: unMantoGeologico		conPresionInicial: unaMedidaDePresion 		yProfunidadDeMetros: unaMedidaDeDistancia ! !Object subclass: #Sum	instanceVariableNames: 'value'	classVariableNames: ''	poolDictionaries: ''	category: 'Tp1-Ing1-Modelo'!!Sum methodsFor: 'Initialize' stamp: 'JuanVanecek 4/5/2017 18:55'!value 	^value! !!Sum methodsFor: 'Initialize' stamp: 'JuanVanecek 4/5/2017 18:54'!initializeOfAll: anAddendsCollection startingWith: aSeed	value := anAddendsCollection 		inject: aSeed		into: [:accumulatedValue :addend | accumulatedValue + addend]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Sum class	instanceVariableNames: ''!!Sum class methodsFor: 'Instance Creation' stamp: 'JuanVanecek 4/5/2017 18:51'!ofAll: aCollection startingWith: aSeed		^self new initializeOfAll: aCollection startingWith: aSeed ! !Object subclass: #Yacimiento	instanceVariableNames: 'tamanio porcentajeDeAgua porcentajeDeGas porcentajeDePetroleo parcelasLibres'	classVariableNames: ''	poolDictionaries: ''	category: 'Tp1-Ing1-Modelo'!!Yacimiento methodsFor: 'Initialize' stamp: 'JuanVanecek 4/5/2017 19:19'!initializeDeTamanioEnHectareas: unasHectareas	compuestoPorAguaEn: unPorcentajeDeAgua	porGasEn: unPorcentajeDeGas	yEnPetroleo: unPorcentajeDePetroleo		tamanio := unasHectareas.	parcelasLibres := unasHectareas.	porcentajeDeAgua := unPorcentajeDeAgua.	porcentajeDeGas := unPorcentajeDeGas.	porcentajeDePetroleo := unPorcentajeDePetroleo! !!Yacimiento methodsFor: 'Accessing' stamp: 'JuanVanecek 4/5/2017 19:14'!parcelaLibreConPresionInicial: unCantidadDePresion 	yProfunidadDeMetros: unaDistanciaEnMetros	^parcelasLibres > 0 		ifTrue: [ 			parcelasLibres := parcelasLibres - 1.			ParcelaDeYacimiento 				conPresionInicial: unCantidadDePresion 				yProfunidadDeMetros: unaDistanciaEnMetros		 ]		ifFalse: [ AssertionFailure signal: 'No hay más parcelas disponibles para este yacimiento' ]! !!Yacimiento methodsFor: 'Accessing' stamp: 'JuanVanecek 4/5/2017 19:37'!parcelaLibreDeTipo: unMantoGeologico	presionInicial: unCantidadDePresion 	yDistanciaAlReservorio: unaDistanciaEnMetros	^parcelasLibres > 0 		ifTrue: [ 			parcelasLibres := parcelasLibres - 1.			ParcelaDeYacimiento 				deTipo: unMantoGeologico				conPresionInicial: unCantidadDePresion 				yProfunidadDeMetros: unaDistanciaEnMetros		 ]		ifFalse: [ AssertionFailure signal: 'No hay más parcelas disponibles para este yacimiento' ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Yacimiento class	instanceVariableNames: ''!!Yacimiento class methodsFor: 'as yet unclassified ' stamp: 'JuanVanecek 4/5/2017 18:48'!deTamanioEnHectareas: unasHectareas	compuestoPorAguaEn: unPorcentajeDeAgua	porGasEn: unPorcentajeDeGas	yEnPetroleo: unPorcentajeDePetroleo		self assertSumanCien: 		(Array with: unPorcentajeDeAgua with: unPorcentajeDeGas with: unPorcentajeDePetroleo).		^self new		initializeDeTamanioEnHectareas: unasHectareas		compuestoPorAguaEn: unPorcentajeDeAgua		porGasEn: unPorcentajeDeGas		yEnPetroleo: unPorcentajeDePetroleo! !!Yacimiento class methodsFor: 'Asserting' stamp: 'JuanVanecek 4/5/2017 18:55'!assertSumanCien: aNumberCollection 	(Sum ofAll: aNumberCollection startingWith: 0) value = 100 		ifFalse: [AssertionFailure signal: 'Los compuestos tienen que sumar el 100% del reservorio'].! !